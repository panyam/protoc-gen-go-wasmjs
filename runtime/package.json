{
  "name": "@protoc-gen-go-wasmjs/runtime",
  "version": "0.0.21",
  "description": "Runtime utilities for protoc-gen-go-wasmjs generated code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.mjs",
      "require": "./dist/browser/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.mjs",
      "require": "./dist/schema/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "protobuf",
    "grpc",
    "wasm",
    "typescript",
    "code-generation"
  ],
  "author": "Sri Panyam",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/panyam/protoc-gen-go-wasmjs.git",
    "directory": "runtime-package"
  },
  "homepage": "https://github.com/panyam/protoc-gen-go-wasmjs#readme",
  "bugs": {
    "url": "https://github.com/panyam/protoc-gen-go-wasmjs/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false
}
